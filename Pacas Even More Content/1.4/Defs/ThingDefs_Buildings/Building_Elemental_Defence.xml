<?xml version="1.0" encoding="utf-8"?>
<Defs>
    
    <ThingDef ParentName="SK_BuildingBase">
        <defName>EmbrasureElemental_Flame</defName>
        <label>embrasure (flame)</label>
        <description>A waist-high metal wall built for defense. Flame powder provides the embrasure with the necessary blast resistance, and gives it the ability to absorb more blunt damage, but not so good against sharp types of damage.</description>
        <thingClass>SK.Building_Embrasure</thingClass>
        <graphicData>
            <texPath>Things/Building/EmbrasureFlame</texPath>
            <graphicClass>Graphic_Appearances</graphicClass>
            <linkType>CornerFiller</linkType>
            <linkFlags>
                <li>Wall</li>
                <li>Rock</li>
            </linkFlags>
            <damageData>
                <cornerTL>Damage/Corner</cornerTL>
                <cornerTR>Damage/Corner</cornerTR>
                <cornerBL>Damage/Corner</cornerBL>
                <cornerBR>Damage/Corner</cornerBR>
                <edgeTop>Damage/Edge</edgeTop>
                <edgeBot>Damage/Edge</edgeBot>
                <edgeLeft>Damage/Edge</edgeLeft>
                <edgeRight>Damage/Edge</edgeRight>
            </damageData>
        </graphicData>
        <uiIconPath>Things/Building/EmbrasureFlame/EmbrasureSmooth_MenuIcon</uiIconPath>
        <altitudeLayer>Building</altitudeLayer>
        <pathCost>85</pathCost>
        <passability>Impassable</passability>
        <castEdgeShadows>true</castEdgeShadows>
        <fillPercent>0.72</fillPercent>
        <tickerType>Rare</tickerType>
        <statBases>
            <MaxHitPoints>1000</MaxHitPoints>
            <Flammability>0</Flammability>
            <Beauty>1</Beauty>
            <WorkToBuild>1200</WorkToBuild>
        </statBases>
        <stuffCategories>
            <li>StrongMetallic</li>
        </stuffCategories>
        <costStuffCount>10</costStuffCount>
        <costList>
            <ComponentIndustrial>2</ComponentIndustrial>
            <ReinforcedConcrete>5</ReinforcedConcrete>
            <LivingFirePowder>1</LivingFirePowder>
        </costList>
        <building>
            <ai_chillDestination>false</ai_chillDestination>
            <isInert>true</isInert>
            <canPlaceOverWall>true</canPlaceOverWall>
            <blueprintGraphicData>
                <texPath>Things/Building/Embrasure/Embrasure_Blueprint_Atlas</texPath>
            </blueprintGraphicData>
            <buildingTags>
                <li>ForceBuildDuringSiege</li>
            </buildingTags>
        </building>
        <placingDraggableDimensions>1</placingDraggableDimensions>
        <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
        <rotatable>false</rotatable>
        <coversFloor>true</coversFloor>
        <holdsRoof>false</holdsRoof>
        <neverMultiSelect>true</neverMultiSelect>
        <useStuffTerrainAffordance>true</useStuffTerrainAffordance>
        <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
        <designationCategory>Security</designationCategory>
        <staticSunShadowHeight>0.20</staticSunShadowHeight>
        <repairEffect>ConstructDirt</repairEffect>
        <disableImpassableShotOverConfigError>true</disableImpassableShotOverConfigError>
        <damageMultipliers Inherit="False">
            <li>
                <damageDef>Bomb</damageDef>
                <multiplier>0.8</multiplier>
            </li>
            <li>
                <damageDef>Bomb_Secondary</damageDef>
                <multiplier>0.8</multiplier>
            </li>
            <li>
                <damageDef>Bullet</damageDef>
                <multiplier>0.5</multiplier>
            </li>
            <li>
                <damageDef>Arrow</damageDef>
                <multiplier>0.3</multiplier>
            </li>
            <li>
                <damageDef>Beanbag</damageDef>
                <multiplier>0.2</multiplier>
            </li>
            <li>
                <damageDef>Optic</damageDef>
                <multiplier>0.5</multiplier>
            </li>
            <li>
                <damageDef>Microwave</damageDef>
                <multiplier>0.7</multiplier>
            </li>
            <li>
                <damageDef>Blunt</damageDef>
                <multiplier>0.5</multiplier>
            </li>
            <li>
                <damageDef>Cut</damageDef>
                <multiplier>1.5</multiplier>
            </li>
            <li>
                <damageDef>Bite</damageDef>
                <multiplier>1.5</multiplier>
            </li>
            <li>
                <damageDef>KineticPulse</damageDef>
                <multiplier>0.7</multiplier>
            </li>
        </damageMultipliers>
        <researchPrerequisites>
            <li>IEDs</li>
            <li>Chemistry_C3</li>
        </researchPrerequisites>
        <constructionSkillPrerequisite>13</constructionSkillPrerequisite>
        <modExtensions>
            <li Class="SK.EmbrasureExtension"/>
        </modExtensions>
    </ThingDef>

